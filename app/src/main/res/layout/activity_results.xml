<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".results"
    android:background="@drawable/aca">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/res"
            android:layout_width="match_parent"
            android:layout_height="113dp"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/coolts"
            tools:layout_editor_absoluteX="26dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <ImageView
            android:id="@+id/snap"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/addd" />

        <ImageView
            android:id="@+id/al"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/aliexp" />

        <ImageView
            android:id="@+id/eba"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/ebay" />

        <ImageView
            android:id="@+id/wak"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/ilff" />

        <ImageView
            android:id="@+id/py"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/paytm" />

        <ImageView
            android:id="@+id/amz"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/rename" />

        <ImageView
            android:id="@+id/fpl"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/sampl" />

        <ImageView
            android:id="@+id/tget"
            android:layout_width="match_parent"
            android:layout_height="106dp"
            android:layout_marginTop="20dp"
            android:layout_weight="1"
            android:visibility="invisible"
            app:srcCompat="@drawable/csc" />


    </LinearLayout>

    <android.widget.Button
        android:id="@+id/comp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/rett"
        android:text="Individual results"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/cmp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <android.widget.Button
        android:id="@+id/cmp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/rett"
        android:text="Compare with your favorite website"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/pmp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <android.widget.Button
        android:id="@+id/pmp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rett"
        android:text="Start next comparison"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/finale"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:autoLink="all"
        android:clickable="true"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:linksClickable="true"
        android:text="Loading please wait.."
        android:textColor="#000000"
        android:textColorLink="#03A9F4"
        android:textIsSelectable="true"
        android:textSize="20sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/comp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

</androidx.constraintlayout.widget.ConstraintLayout>